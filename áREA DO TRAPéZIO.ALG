Algoritmo "área do trapézio"

Var

  b,_B,h:inteiro
  area:real
  
     


Inicio

   escreval("digite o valor de b")
          leia(b)
   escreval("digite o valor de B")
          leia(_B)
   escreval("digite o valor de h")
          leia(h)
          area <- ((b+_B)/2)*h)
          escreval("a área do trapézio é igual a ",area)
          
          

Fimalgoritmo